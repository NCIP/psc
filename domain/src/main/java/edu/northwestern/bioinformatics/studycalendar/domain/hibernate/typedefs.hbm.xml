<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <typedef class="edu.northwestern.bioinformatics.studycalendar.domain.tools.hibernate.EnumUserType" name="durationUnit">
        <param name="enumClassName">edu.northwestern.bioinformatics.studycalendar.domain.Duration$Unit</param>
    </typedef>
    <typedef class="edu.northwestern.bioinformatics.studycalendar.domain.tools.hibernate.ControlledVocabularyObjectType" name="scheduledActivityMode">
        <param name="enumClass">edu.northwestern.bioinformatics.studycalendar.domain.ScheduledActivityMode</param>
    </typedef>
    <typedef class="edu.northwestern.bioinformatics.studycalendar.domain.tools.hibernate.StringCodedEnumType" name="gender">
        <param name="enumClass">edu.northwestern.bioinformatics.studycalendar.domain.Gender</param>
    </typedef>
</hibernate-mapping>