<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <typedef class="edu.northwestern.bioinformatics.studycalendar.utils.hibernate.EnumUserType" name="durationUnit">
        <param name="enumClassName">edu.northwestern.bioinformatics.studycalendar.domain.Duration$Unit</param>
    </typedef>
    <typedef class="edu.northwestern.bioinformatics.studycalendar.utils.hibernate.ControlledVocabularyObjectType" name="scheduledEventMode">
        <param name="enumClass">edu.northwestern.bioinformatics.studycalendar.domain.ScheduledEventMode</param>
    </typedef>
    <typedef class="edu.northwestern.bioinformatics.studycalendar.utils.hibernate.ControlledVocabularyObjectType" name="activityType">
        <param name="enumClass">edu.northwestern.bioinformatics.studycalendar.domain.ActivityType</param>
    </typedef>
    <typedef class="edu.northwestern.bioinformatics.studycalendar.utils.hibernate.StringCodedEnumType" name="userRole">
         <param name="enumClass">edu.northwestern.bioinformatics.studycalendar.domain.Role</param>
    </typedef>
</hibernate-mapping>