<?xml version="1.0"?>
<!DOCTYPE jasperReport
  PUBLIC "-//JasperReports//DTD Report Design//EN"
  "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">
<jasperReport name="generateReport" pageWidth="795">
	<style name="Arial_Normal_Default" isDefault="true" fontName="Arial" fontSize="8" isBold="false" isItalic="false" isUnderline="false" isStrikeThrough="false" pdfFontName="Helvetica"/>
	<style name="Arial_Normal_Header" isDefault="false" fontName="Arial" fontSize="10" isBold="true" isItalic="false" isUnderline="false" isStrikeThrough="false" pdfFontName="Helvetica-Bold"/>
	<style name="Arial_Bold" isDefault="false" fontName="Arial" fontSize="18" isBold="true" isItalic="false" isUnderline="false" isStrikeThrough="false" pdfFontName="Helvetica-Bold"/>
	<field name="dateStr" class="java.lang.String"/>
	<field name="idealDateStr" class="java.lang.String"/>
	<field name="studyName" class="java.lang.String"/>
	<field name="siteName" class="java.lang.String"/>
	<field name="participantName" class="java.lang.String"/>
	<field name="eventName" class="java.lang.String"/>
	<field name="currentState" class="java.lang.String"/>
	<field name="epochName" class="java.lang.String"/>
	<field name="armName" class="java.lang.String"/>
<title>
	<band height="70">
		<staticText>
			<reportElement width="700" height="50" x="0" y="0" style="Arial_Bold"/>
			<text>
				<![CDATA[caBIG PSC Report]]>
			</text>
		</staticText>
	</band>
 </title>
<pageHeader>
	<band height="40">
		<staticText>
			<reportElement width="70" height="40" x="0" y="0" style="Arial_Normal_Header"/>
			<text>
				<![CDATA[Scheduled Date]]>
			</text>
		</staticText>
		<staticText>
			<reportElement width="70" height="40" x="70" y="0" style="Arial_Normal_Header"/>
			<text>
				<![CDATA[Study]]>
			</text>
		</staticText>
		<staticText>
			<reportElement width="70" height="40" x="140" y="0" style="Arial_Normal_Header"/>
			<text>
				<![CDATA[Site]]>
			</text>
		</staticText>
		<staticText>
			<reportElement width="70" height="40" x="210" y="0" style="Arial_Normal_Header"/>
			<text>
				<![CDATA[Paticipant]]>
			</text>
		</staticText>
		<staticText>
			<reportElement width="70" height="40" x="280" y="0" style="Arial_Normal_Header"/>
			<text>
				<![CDATA[Event]]>
			</text>
		</staticText>
		<staticText>
			<reportElement width="70" height="40" x="350" y="0" style="Arial_Normal_Header"/>
			<text>
				<![CDATA[Event State]]>
			</text>
		</staticText>
		<staticText>
			<reportElement width="70" height="40" x="420" y="0" style="Arial_Normal_Header"/>
			<text>
				<![CDATA[Epoch]]>
			</text>
		</staticText>
		<staticText>
			<reportElement width="70" height="40" x="490" y="0" style="Arial_Normal_Header"/>
			<text>
				<![CDATA[Arm]]>
			</text>
		</staticText>
		<staticText>
			<reportElement width="70" height="40" x="560" y="0" style="Arial_Normal_Header"/>
			<text>
				<![CDATA[Ideal Date]]>
			</text>
		</staticText>
	</band>
</pageHeader>
<detail>
	<band height="20">
		<textField>
			<reportElement width="70" height="20" x="0" y="0"/>
			<textFieldExpression><![CDATA[$F{dateStr}]]></textFieldExpression>
		</textField>
		<textField>
			<reportElement width="70" height="20" x="75" y="0"/>
			<textFieldExpression><![CDATA[$F{studyName}]]></textFieldExpression>
		</textField>
		<textField>
			<reportElement width="70" height="20" x="145" y="0"/>
			<textFieldExpression><![CDATA[$F{siteName}]]></textFieldExpression>
		</textField>
		<textField>
			<reportElement width="70" height="20" x="215" y="0"/>
			<textFieldExpression><![CDATA[$F{participantName}]]></textFieldExpression>
		</textField>
		<textField>
			<reportElement width="70" height="20" x="285" y="0"/>
			<textFieldExpression><![CDATA[$F{eventName}]]></textFieldExpression>
		</textField>
		<textField>
			<reportElement width="70" height="20" x="355" y="0"/>
			<textFieldExpression><![CDATA[$F{currentState}]]></textFieldExpression>
		</textField>
		<textField>
			<reportElement width="70" height="20" x="425" y="0"/>
			<textFieldExpression><![CDATA[$F{epochName}]]></textFieldExpression>
		</textField>
		<textField>
			<reportElement width="70" height="20" x="495" y="0"/>
			<textFieldExpression><![CDATA[$F{armName}]]></textFieldExpression>
		</textField>
		<textField>
			<reportElement width="70" height="20" x="565" y="0"/>
			<textFieldExpression><![CDATA[$F{idealDateStr}]]></textFieldExpression>
		</textField>
	</band>
</detail>
</jasperReport>