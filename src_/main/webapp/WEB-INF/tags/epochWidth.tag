<%@tag%><%@taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@attribute name="epoch" required="true" type="edu.northwestern.bioinformatics.studycalendar.domain.Epoch"%><c:set var="len" value="${fn:length(epoch.plannedCalendar.epochs)}"/><c:choose><c:when test="${len == 1}">48</c:when><c:otherwise>${100 / len - 2}</c:otherwise></c:choose>%