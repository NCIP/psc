        />
    <!-- with the same key as another amendment (-223) from a different study -->
    <AMENDMENTS
        id="-225"
        amendment_date="2008-11-23"
        name="turkey"
        previous_amendment_id="-224"
        mandatory="0"
        />

    <DELTAS
