<?xml version='1.0' encoding='utf-8'?>

<!-- General settings for DA-Launcher -->
<general-settings>
	
	<!--
	Thread Context Class Loader settings. Supported attributes:
		osgi-aware - If true DA-Launcer will set the Thread Context Class Loader to a Class Loader which loads classes and resources from the OSGi Environment. Default value is 'false'.
	-->	
	<thread-context-class-loader osgi-aware="false" />
	
</general-settings>
