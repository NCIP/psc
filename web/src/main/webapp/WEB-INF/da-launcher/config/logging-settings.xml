<?xml version='1.0' encoding='utf-8'?>

<!-- Logging settings for DA-Launcher -->
<logger-settings>
	
	<!--
	The logger which logs bundle lifecycle events: bundle installed, updated, uninstalled, etc. Supported attributes:
		enabled - Determines whether the logger is enabled or no. true - if the logger should log events, otherwise false. Default value is 'true'.
	-->
	<bundle-lifecycle-events-logger enabled="true" />
	
	<!--
	The logger which logs DA-Launcher events: Launching, Launched, Shutdown, etc. Supported attributes:
		enabled - Determines whether the logger is enabled or no. true - if the logger should log events, otherwise false. Default value is 'true'.
	-->
	<system-events-logger enabled="true" />
	
</logger-settings>
