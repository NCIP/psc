 <ns3:Services xsi:type="ns3:ServicesType" xmlns:ns3="gme://gov.nih.nci.cagrid.introduce/1/Services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ns3:Service name="StudyService" namespace="http://grid.studycalendar.bioinformatics.northwestern.edu/StudyService" packageName="edu.northwestern.bioinformatics.studycalendar.grid" resourceFrameworkType="main" xsi:type="ns3:ServiceType">
   <ns4:Methods xsi:type="ns4:MethodsType" xmlns:ns4="gme://gov.nih.nci.cagrid.introduce/1/Methods">
    <ns4:Method isImported="true" isProvided="true" name="getServiceSecurityMetadata" xsi:type="ns4:MethodType">
     <ns4:Output qName="ns5:ServiceSecurityMetadata" isArray="false" isClientHandle="false" xmlns:ns5="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"/>
     <ns4:ImportInformation namespace="http://security.introduce.cagrid.nci.nih.gov/ServiceSecurity" packageName="gov.nih.nci.cagrid.introduce.security.stubs" portTypeName="ServiceSecurityPortType" wsdlFile="ServiceSecurity.wsdl"/>
     <ns4:ProviderInformation providerClass="gov.nih.nci.cagrid.introduce.security.service.globus.ServiceSecurityProviderImpl"/>
    </ns4:Method>
    <ns4:Method isImported="false" isProvided="false" name="retrieveStudyByAssignedIdentifier" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="xsd:string" isArray="false" name="assignedIdentifier" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
     </ns4:Inputs>
     <ns4:Output qName="ns6:study" isArray="false" xmlns:ns6="http://bioinformatics.northwestern.edu/ns/psc"/>
     <ns4:Exceptions>
      <ns4:Exception name="StudyDoesNotExistsException" qname="ns7:StudyDoesNotExistsException" xmlns:ns7="http://grid.studycalendar.bioinformatics.northwestern.edu/StudyService/types"/>
     </ns4:Exceptions>
    </ns4:Method>
    <ns4:Method isImported="false" isProvided="false" name="createStudy" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns8:study" isArray="false" name="study" xmlns:ns8="http://bioinformatics.northwestern.edu/ns/psc"/>
     </ns4:Inputs>
     <ns4:Output qName="ns9:study" isArray="false" xmlns:ns9="http://bioinformatics.northwestern.edu/ns/psc"/>
     <ns4:Exceptions>
      <ns4:Exception name="StudyAlreadyExistsException" qname="ns10:StudyAlreadyExistsException" xmlns:ns10="http://grid.studycalendar.bioinformatics.northwestern.edu/StudyService/types"/>
     </ns4:Exceptions>
    </ns4:Method>
   </ns4:Methods>
   <ns11:ResourcePropertiesList xsi:type="ns11:ResourcePropertiesListType" xmlns:ns11="gme://gov.nih.nci.cagrid.introduce/1/Resources">
